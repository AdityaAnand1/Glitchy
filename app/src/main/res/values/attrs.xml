<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GlithcyTextView">
        <attr name="glitchyTypeface" format="enum" >
            <enum name="regular" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="light" value="2"/>
            <enum name="mono" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="BarView">
        <attr name="barViewProgress" format="integer" />
        <attr name="barViewColor" format="color"/>
        <attr name="barViewBackgroundColor" format="color"/>
        <attr name="barViewThickness" format="dimension" />
        <attr name="barViewCap" format="enum">
            <enum name="square" value="0"/>
            <enum name="round" value="1"/>
        </attr>
    </declare-styleable>
</resources>